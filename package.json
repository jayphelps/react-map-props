{
  "name": "react-map-props",
  "version": "0.1.0",
  "description": "Transform your component props",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/react-map-props.js && NODE_ENV=production webpack src/index.js dist/react-map-props.min.js",
    "test": "NODE_ENV=test mocha",
    "test:watch": "NODE_ENV=test mocha --watch",
    "test:cov": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha",
    "prepublish": "npm run test && npm run clean && npm run build && npm run build:umd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jayphelps/react-map-props.git"
  },
  "keywords": [
    "react",
    "transform",
    "props",
    "map",
    "componentWillReceiveProps"
  ],
  "author": "Jay Phelps <hello@jayphelps.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jayphelps/react-map-props/issues"
  },
  "homepage": "https://github.com/jayphelps/react-map-props",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "expect": "^1.18.0",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.3.0",
    "rimraf": "^2.5.2",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "react": "*"
  }
}
